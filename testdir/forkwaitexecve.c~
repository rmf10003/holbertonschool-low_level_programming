#include <stdio.h>
#include <unistd.h>
#include <sys/types.h>
#include <sys/wait.h>

/**
 * main - fork & wait example
 *
 * Return: Always 0.
 */
int main(int ac, char **av)
{
	pid_t child_pid;
	int status;
	int i;

	child_pid = fork();
	for (i = 0; i < 5; i++)
	{
	if (child_pid == 0)
		execve(argv[0], argv, NULL);
	else
		wait(&status);
	}
	return (0);
}

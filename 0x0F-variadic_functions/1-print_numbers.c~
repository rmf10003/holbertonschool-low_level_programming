/**
 * print_numbers - print numbers followed by a new line
 * @separator: pointer to a constant char
 * @n: number of args passed to print_numbers
 *
 * Return: nothing
 */
void print_numbers(const char *separator, const unsigned int n, ...)
{
	unsigned int i;

	if (separator == NULL)
		separator = "";
	va_list numbs;
	va_start(numbs, n);
	for(i = 0; i < n; i++)
	{
		printf("%d", va_args);
		if (i < n - 1)
			printf("%s", separator);
		else
			va_end(numbs);
}
